<div class="password-veil inactive"></div>
<div class="validator form-wrapper inactive">
  <form method="POST">
    <input id='protected-action' type="hidden" name="action">
    <input type="hidden" name="protectedId" id="form-id">

    <div class="form-item">
      <div class="form-label">
        <img src="/icons/warning.png" alt="">
        <label for="password">Insert admin password</label>
      </div>
      <input type="password" name="password" id="password" required>
    </div>

    <div class="validator-buttons">
      <button class="cancel-validation-btn" type="button">Cancel</button>
      <button class="submit-btn" type="submit">Commit</button>
    </div>
  </form>
</div>
